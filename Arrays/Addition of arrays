#include <iostream>
using namespace std;

int main() {
    int A[2][2] = {{1, 2}, {4, 5}};
    int B[2][2]={{10,16},{11,7}};
    int sum[2][2];
    for(int i = 0; i < 2; i++)
        for(int j = 0; j < 2; j++)
           sum[i][j]=A[i][j]+B[i][j]; 

    cout << "Sum is:\n";
    for(int i = 0; i < 2; i++) {
        for(int j = 0; j < 2; j++)
            cout <<sum[i][j]<<" ";
        cout << endl;
    }

    return 0;
}

